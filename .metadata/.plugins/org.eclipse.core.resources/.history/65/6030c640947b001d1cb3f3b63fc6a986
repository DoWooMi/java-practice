		//쿠키저장 함수
		function setCookie_T( name, value) { 
			document.cookie = name + "=" + escape( value ) + "; path=/;" 
		}

		$(document).ready(function(){
			$("#top_banner .top_banner_close").click(function(){			
				setCookie_T( "topBanner", "tt"); 		
				$('#top_banner').slideUp(500); 
			});
		});

	//-->  
</script>

