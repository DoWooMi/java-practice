	$('button.btn_wish').each(function (index, item) { 		
		var stringVal = "",
		substring = $(this).attr('data-it_id');
		if(stringVal.indexOf(substring) !== -1){
			$(this).find('i').css("color","#C53428");
		}		
	});
	
	$('button.btn_cart').each(function (index, item) { 		
